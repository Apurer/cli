{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "serve in memory",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/temporal",
      "cwd": "${workspaceFolder}",
      "args": [
        "server",
        "start-dev",
        "--namespace",
        "default",
      ]
    },
    {
      "name": "workflow list",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/temporal",
      "cwd": "${workspaceFolder}",
      "args": [
        "workflow",
        "list",
        "--env",
        "canary",
      ]
    }
  ],
}
